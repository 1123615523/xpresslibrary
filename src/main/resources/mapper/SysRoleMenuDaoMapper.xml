<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.dao.SysRoleMenuDao">

    <insert id="add">
        insert into sys_role_menu values(#{rid},#{pid})
    </insert>
    <delete id="del">
        delete from sys_role_menu where rid = #{rid}
    </delete>
</mapper>