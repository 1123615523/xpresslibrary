<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--接口全路径-->
<mapper namespace="com.aaa.dao.CustomerDao">
    <insert id="add" useGeneratedKeys="true" keyProperty="customerid">
        insert into customerinfo(customerid,customerpwd,customeremail,customertel)
        values(#{customerid},#{customerpwd},#{customeremail},#{customertel})
    </insert>


</mapper>